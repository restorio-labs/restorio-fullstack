{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020"],
      "module": "ESNext",
      "moduleResolution": "node",
  
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
  
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  